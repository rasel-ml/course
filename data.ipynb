   "source": [
    "n=int(input(\"Type a number:\"))\n",
    "prime=True\n",
    "for i in range (2,n):\n",
    "    if n%i==0:\n",
    "        prime=False\n",
    "        break\n",
    "if prime:\n",
    "    print(n,\"is a prime number.\")\n",
    "else:\n",
    "    print(n,\"is not a prime number.\")"
   ]
